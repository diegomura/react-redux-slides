@-webkit-keyframes fadeIn { from { opacity:0; } to { opacity:1; } }
@-moz-keyframes fadeIn { from { opacity:0; } to { opacity:1; } }
@keyframes fadeIn { from { opacity:0; } to { opacity:1; } }

@-webkit-keyframes enlarge { from { width: 0px; } to { width: 350px; } }
@-moz-keyframes enlarge { from { width: 0px; } to { width: 350px; } }
@keyframes enlarge { from { width: 0px; } to { width: 350px; } }

@-webkit-keyframes radius-enlarge { from { opacity: 0; transform: scale(3) } to { opacity: 1; transform: scale(1) } }
@-moz-keyframes radius-enlarge { from { opacity: 0; transform: scale(3) } to { opacity: 1; transform: scale(1) } }
@keyframes radius-enlarge { from { opacity: 0; transform: scale(3) } to { opacity: 1; transform: scale(1) } }
